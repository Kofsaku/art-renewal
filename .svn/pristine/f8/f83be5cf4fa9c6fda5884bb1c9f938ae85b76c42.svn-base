package jp.co.art.trident.model.form;

import java.io.Serializable;
import java.util.List;

import jp.co.art.trident.model.entity.GateEdit;
import jp.co.art.trident.model.entity.MCT;

/**
 * <b>ゲート編集画面クラス.</b> <br/>
 * 画面情報を保持します<br/>
 * <br/>
 * 
 * <pre>
 * </pre>
 */	
public class GateEditForm implements Serializable{

	private static final long serialVersionUID = 1434976356359994371L;

	/** ゲート編集リスト */
	private List<GateEdit> mstGateList;
	
	/** 連続解錠時間帯リスト */
	private List<MCT> mstCTList;
	
	/** ゲート番号 */
	private String gateno;
	
	/** 装置タイプ */
	private String hosttype;
	
	/** ホスト番号 */
	private String hostno;
	
	/** アドレス番号 */
