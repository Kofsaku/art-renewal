	/** ゲート名称 */
	private String gatename;
	
	/** ゲートタイプ */	
	private int gatetype_4;	
	
	/** 連続解錠時間番号 */
	private String ctimer;
	
	/** 連続解錠時間名称 */
	private String ctimername;
	
	/** ゲート存在フラグ */
	private boolean gateflag;
	
	/** メッセージ */
	private String message;
	
	/** アップロード件数 */
	private int uploadCount;

	/** 処理件数 */
	private int updateCount;
	
	/** エラー件数 */
	private int errorCount;

	/** エラーリスト */
	private List<String> errorList;
	
	//　以下、ゲッター、セッター　-----------------------
	public List<GateEdit> getMstGateList() {
		return mstGateList;
	}
	public void setMstGateList(List<GateEdit> mstGateList) {
		this.mstGateList = mstGateList;
	}
	public List<MCT> getMstCTList() {
		return mstCTList;
	}
	public void setMstCTList(List<MCT> mstCTList) {
		this.mstCTList = mstCTList;
	}
	public String getGateno() {
		return gateno;
	}
	public void setGateno(String gateno) {
		this.gateno = gateno;
	}
	public String getHosttype() {
		return hosttype;
	}
	public void setHosttype(String hosttype) {
		this.hosttype = hosttype;
	}
	public String getHostno() {
		return hostno;
	}
	public void setHostno(String hostno) {
		this.hostno = hostno;
	}
	public String getAddressno() {
		return addressno;
	}
	public void setAddressno(String addressno) {
		this.addressno = addressno;
	}	
	public String getGatename() {
		return gatename;
	}
	public void setGatename(String gatename) {
		this.gatename = gatename;
	}
	public int getGatetype_4() {
		return gatetype_4;
	}
