package jp.co.art.trident.model.entity;

import java.io.Serializable;

/**ユーザーマスタを保持するクラス**/

public class MUser implements Serializable {

	private static final long serialVersionUID = 2825413360039081255L;
	/**
     * デフォルトコンストラクタ
     */
	public MUser() {};
	
	/**
     * デフォルトコンストラクタ
     */
	public MUser(String username,String password
			,int level,String entrytime
			,String uptime,String authority
			,String tenantno
			) {
		this.username = username;
		this.password = password;
		this.level = level;
		this.entrytime = entrytime;
		this.uptime = uptime;
		this.authority = authority;
		this.tenantno = tenantno;
	};
	
	/** ユーザ名 */
	private String username;
	/** パスワード */
	private String password;
	/** レベル */
	private int level;
	/** 登録日時 */
	private String entrytime;
	/** 変更日時 */
	private String uptime;
	/** 権限 */
	private String authority;
	/** テナント番号 */
	private String tenantno;
	
	//　以下、ゲッター、セッター　-----------------------
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public int getLevel() {
		return level;
	}
	public void setLevel(int level) {
		this.level = level;
	}	
	public String getEntrytime() {
		return entrytime;
	}
	public void setEntrytime(String entrytime) {
		this.entrytime = entrytime;
	}	
	public String getUptime() {
		return uptime;
	}
	public void setUptime(String uptime) {
		this.uptime = uptime;
	}
	public String getAuthority() {
		return authority;
	}
	public void setAuthority(String authority) {
		this.authority = authority;
	}
	public String getTenantno() {
		return tenantno;
	}
	public void setTenantno(String tenantno) {
		this.tenantno = tenantno;
	}
}
