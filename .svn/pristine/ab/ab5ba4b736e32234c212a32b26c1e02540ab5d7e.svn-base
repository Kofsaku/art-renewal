package jp.co.art.trident.model;

import java.io.Serializable;
import java.util.Collection;

import org.springframework.data.annotation.Id;
import org.springframework.security.core.GrantedAuthority;

/**ユーザ情報を保持するクラス**/
public class User implements Serializable{

	private static final long serialVersionUID = 6925392563431973569L;

	/**
	 * デフォルトコンストラクタ
	 */
	public User() {};
	
	/**
	 * コンストラクタ
	 * @param userName
	 * @param password
	 * @param level
	 * @param entrytime
	 * @param uptime
	 * @param authority
	 * @param tenantno
	 */	
	public User(String userName, String password, int level, String entrytime, String uptime, String authority,String tenantno
			,Collection<? extends GrantedAuthority> authorities
			,String initialMenuDispFlg, String initialDispFlg, String gateRegistrationDispFlg
			,String deviceRegistrationDispFlg, String tenantRegistrationDispFlg, String floorRegistrationDispFlg
			,String groupControlRegistrationDispFlg, String errorPopupSettingsDispFlg
			,int filterflag) {
		this.userName = userName;
		this.password = password;	
		this.level = level;	
		this.entrytime = entrytime;	
		this.uptime = uptime;	
		this.authority = authority;	
		this.tenantno = tenantno;		
		this.authorities = authorities;
		this.initialMenuDispFlg = initialMenuDispFlg;
		this.initialDispFlg = initialDispFlg;
		this.gateRegistrationDispFlg = gateRegistrationDispFlg;
		this.deviceRegistrationDispFlg = deviceRegistrationDispFlg;
		this.tenantRegistrationDispFlg = tenantRegistrationDispFlg;
		this.floorRegistrationDispFlg = floorRegistrationDispFlg;
		this.groupControlRegistrationDispFlg = groupControlRegistrationDispFlg;
		this.errorPopupSettingsDispFlg = errorPopupSettingsDispFlg;
		this.filterflag = filterflag;
		
	}
	
	/*****************************
	 * DB情報
	 ****************************/
	/** ユーザ名. */
	@Id
	private String userName;
	/** パスワード */
	private String password;
	/** レベル */
	private int level;
	/** 登録日時 */
	private String entrytime;
	/** 変更日時 */
	private String uptime;
	/** 権限 */
	private String authority;
	/** テナント番号 */
	private String tenantno;

	/*****************************
	 * 独自情報
	 ****************************/
	/** ロール権限 */
	private Collection<? extends GrantedAuthority> authorities;	
	
	/** レベル9の場合　権限の値によって、メニュー画面の表示・非表示を切り替える */
	/** 初期設定メニュー */
	private String initialMenuDispFlg;
	
	/** 初期設定 */
	private String initialDispFlg;	
	
	/** ゲート登録 */
	private String gateRegistrationDispFlg;	

	/** 装置登録 */
	private String deviceRegistrationDispFlg;	
	
	/** テナント登録 */
	private String tenantRegistrationDispFlg;	
	
	/** フロア登録 */
	private String floorRegistrationDispFlg;	
	
	/** 一斉制御グループ登録 */
	private String groupControlRegistrationDispFlg;	
	
	/** エラーポップアップ設定 */
	private String errorPopupSettingsDispFlg;	
	
	/** 個人登録 */
	private String individualRegistrationDispFlg;	
	
	/** ゲート編集 */
	private String gateEditDispFlg;	
	
	/** 報告書 */
